{
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "require": {
        "symfony/http-foundation": "^7.3",
        "symfony/http-client": "^7.3"
    },
    "require-dev": {
        "pestphp/pest": "^4.1.3",
        "symfony/var-dumper": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
